{
    "name": ":vendor/:package_name",
    "description": ":package_description",
    "keywords": [
        ":vendor",
        ":package_name"
    ],
    "homepage": "https://github.com/jbenner-radham/twine",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "james.benner@gmail.com",
            "homepage": "http://www.radioactivehamster.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "RadHam\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\:package_name\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^3.0",
        "codeception/codeception": "^2.1",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.0"
    }
}
